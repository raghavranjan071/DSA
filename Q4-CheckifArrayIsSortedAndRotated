#include <bits/stdc++.h>
using namespace std;

bool f(vector<int> arr)
{
    int n = arr.size();
    int ct = 0;
    for (int i = 1; i < n; i++)
    {
        if (arr[i-1] > arr[i%n])
        {
            ct++;
        }
    }

    if (arr[0] < arr[n - 1])
    {
        ct++;
    }
    return ct<=1;

}

int main()
{
    int n;
    cin >> n;
    vector<int> arr(n);
    for(int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    int result = f(arr);
    cout << "Result: " << result << endl;

    return 0;
}